{
  "manifest_version": 3,
  "name": "Wikidata One Click Info",
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "1": "Icon/Wiki_logo_3_update.png"
    },
    "default_title": "Wikidata One Click Info",
    "default_popup": "Popup/popup.html"
  },

  "description": "This is an extension to look up data from Wikidata",
  "icons": {
    "5": "Icon/1024px-Wikidata-logo.svg.png",
    "32": "Icon/2872375-32.png",
    "64": "Icon/2872375-64.png",
    "128": "Icon/2872375-128.png"
  },
  "author": "Wikidata OCI team",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["page.js", "content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
